{
	"Schema":{
		"type":"object",
		"properties":{
            "solvers":{
                "type":"array",
                "title":"solvers",
                "items":{
                    "type":"object",
                    "properties":{
                        "parameterName":{
                            "type":"string",
                            "title":"parametr name",
                            "description":"name of the parameter - P|U|k|omega|epsilon and etc."
                        },
                        "parameterProperties":{
                            "type":"object",
                            "properties":{
                                "solver":{      
                                    "type":"string",
                                    "title":"solver",
                                    "enum":[
                                        "PCG",
                                        "PCG/PBiCGStab",
                                        "PCG/PBiCG",
                                        "smoothSolver",
                                        "GAMG",
                                        "diagonal"
                                    ]
                                },
                                "preconditioner":{
                                    "type":"string",
                                    "title":"preconditioner",
                                    "enum":[
                                        "none",
                                        "DIC",
                                        "DILU",
                                        "FDIC",
                                        "diagonal",
                                        "GAMG"
                                    ]
                                },
                                "smoother":{
                                    "type":"string",
                                    "title":"smoother",
                                    "enum":[
                                        "none",
                                        "GaussSeidel",
                                        "symGaussSeidel",
                                        "DIC",
                                        "DILU",
                                        "DICGaussSeidel"
                                    ]
                                },
                                "tolerance":{
                                    "type":"number",
                                    "title":"tolerance",
                                    "description":"the solver tolerance"
                                },
                                "relTol":{
                                    "type":"number",
                                    "title":"relTol",
                                    "description":"the solver relative tolerance"
                                },
                                "GAMGproperties":{
                                    "type":"array",
                                    "title":"GAMG properties",
                                    "items":{
                                        "type":"object",
                                        "properties":{
                                            "Template": "openFOAM.pimpleFOAM.WebGui.FvSolution.GAMGproperties"
                                        }
                                    }
                                },
                                "SmoothProperties":{
                                    "type":"array",
                                    "title":"Smooth Properties",
                                    "items":{
                                        "type":"object",
                                        "properties":{
                                            "Template":"openFOAM.pimpleFOAM.WebGui.FvSolution.SmoothProperties"
                                        }
                                    }
                                },
                                "more properties":{
                                    "type":"object",
                                    "additionalProperties": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
                
            },
            "algorithms":{
                "type":"array",
                "title":"PISO, SIMPLE or PIMPLE algorithms",
                "items":{
                    "type":"object",
                    "anyOf":[
                        {
                            "Template":"openFOAM.pimpleFOAM.WebGui.FvSolution.PISO"
                        },
                        {
                            "Template":"openFOAM.pimpleFOAM.WebGui.FvSolution.SIMPLE"
                        },
                        {
                            "Template":"openFOAM.pimpleFOAM.WebGui.FvSolution.PIMPLE"
                        }
                    ]
                }
            },
            "potentialFlow":{
                "type":"object",
                "title":"potentialFlow",
                "properties":{
                    "nNonOrthogonalCorrectors":{
                        "type":"number",
                        "title":"nNonOrthogonalCorrectors"
                    }
                }
            },
            "relaxationFactors":{
                "type":"object",
                "title":"relaxationFactors",
                "properties":{
                    "Template":"openFOAM.pimpleFOAM.WebGui.FvSolution.relaxationFactors"
                }
            }
        }
    },
	"uiSchema":{ 
    },
    "formData":{ 
        "solvers":[ 
            { 
                "parameterProperties":{ 
                    "GAMGproperties":[ 
                        { 
                            "cacheAgglomeration":true,
                            "agglomerator":"faceAreaPair",
                            "nCellsInCoarsestLevel":10,
                            "mergeLevels":1
                        }
                    ],
                    "solver":"GAMG",
                    "smoother":"GaussSeidel",
                    "tolerance":0,
                    "relTol":0.1
                },
                "parameterName":"p"
            },
            {
                "parameterProperties":{
                    "smoother":"DICGaussSeidel",
                    "tolerance":1e-06,
                    "relTol":0
                },
                "parameterName":"pFinal"
            },
            {
                "parameterProperties":{
                    "solver":"smoothSolver",
                    "smoother":"symGaussSeidel",
                    "tolerance":1e-05,
                    "relTol":0.1
                },
                "parameterName":"(U|k|nuTilda)"
            },
            {
                "parameterProperties":{
                    "tolerance":1e-05,
                    "relTol":0
                },
                "parameterName":"(U|k|nuTilda)Final"
            }
        ],

        "algorithms":[ 
            { 
                "PIMPLEName":"PIMPLE",
                "PIMPLEproperties":{
                    "residualControl":{
                        "P":1e-4,
                        "U":1e-4,
                        "(k|omega|epsilon)":1e-4
                    },
                    "nOuterCorrectorsPIMPLE": 1,
                    "nCorrectorsPIMPLE": 2,
                    "nNonOrthogonalCorrectorsPIMPLE":0,
                    "pRefCellPIMPLE":1001,
                    "pRefValuePIMPLE":0,
                    "momentumPredictorPIMPLE": "yes"
                }
            }
        ],
        "potentialFlow":{ 
            "nNonOrthogonalCorrectors":10
        },
        "relaxationFactors":{ 
            "fields":{
                "P":0.3
            },
            "equations":{
                "U":0.7,
                "k" : 0.7,
                "omega":0.7,
                "epsilon" : 0.7
            }            
        }
    }   
}
