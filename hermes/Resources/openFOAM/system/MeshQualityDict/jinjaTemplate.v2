FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      meshQualityDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

{%- import "openFOAM/utils.jinja" as utils %}
{%- set p = input_parameters %}

{# ----------------------------- #
   Render top-level entries
# ----------------------------- #}
{%- for key, val in p.items() %}
    {%- if key != 'relaxed' %}

        {# Case 1: key is numeric â†’ raw line from reverse parser #}
        {%- if key | int ~ '' == key %}
{{ val }}
        {%- else %}
{{ key }}    {{ val }};
        {%- endif %}

    {%- endif %}
{%- endfor %}

{# ----------------------------- #
   Handle 'relaxed' block
# ----------------------------- #}
{%- if p.relaxed is defined and p.relaxed %}
relaxed
{
    {% for key, val in p.relaxed.items() %}
    {{ key }}    {{ val }};
    {% endfor %}
}
{%- endif %}


