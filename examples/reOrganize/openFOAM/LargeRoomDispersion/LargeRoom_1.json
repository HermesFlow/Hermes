              },
              {
                "name": "ls -d processor* | xargs -i cp -r 0.parallel/* ./{}/0/ $1",
                "parameters": null,
                "couldRunInParallel": false,
                "foamJob": false
              },
              {
                "name": "changeDictionary",
                "parameters": null,
                "couldRunInParallel": true
