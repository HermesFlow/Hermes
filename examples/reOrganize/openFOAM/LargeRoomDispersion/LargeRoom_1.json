                  },
                  "closedDoor": {
                    "type": "zeroGradient"
                  },
                  "roundWindow": {
                    "type": "zeroGradient"
                  }
                }
              },
              "p": {
                "boundaryField": {
                  "Ceiling": {
                    "type": "fixedFluxPressure",
                    "rho": "rhok",
                    "value": "uniform 0"
                  },
                  "Floor": {
                    "type": "fixedFluxPressure",
                    "rho": "rhok",
                    "value": "uniform 0"
                  },
                  "Walls": {
                    "type": "fixedFluxPressure",
                    "rho": "rhok",
                    "value": "uniform 0"
                  },
                  "closedDoor": {
                    "type": "fixedValue",
                    "value": "uniform 0"
                  },
                  "roundWindow": {
                    "type": "zeroGradient"
