        },
        "type": "openFOAM.system.ChangeDictionary"
      },
      "buildAllRun": {
        "Execution": {
          "input_parameters": {
            "casePath": "{Parameters.output.targetDirectory}",
            "caseExecution": {
              "parallelCase": true,
              "slurm": false,
              "getNumberOfSubdomains": 10,
              "runFile": []
            },
            "parallelCase": true,
            "runFile": [
              {
                "name": "blockMesh",
                "couldRunInParallel": false,
                "parameters": null
              },
              {
                "name": "surfaceFeatureExtract",
