                                        "rho": "rhok",
                                        "value": "uniform 0"
                                    },
                                    "closedDoor": {
                                        "type": "fixedValue",
                                        "value": "uniform 0"
                                    },
                                    "roundWindow": {
                                        "type": "zeroGradient"
                                    }
                                }
                            }
                        }
                    }
                },
                "type": "openFOAM.system.ChangeDictionary"
            },
            "buildAllRun": {
                "Execution": {
                    "input_parameters": {
                        "casePath": "{Parameters.output.targetDirectory}",
                        "caseExecution": {
                            "parallelCase": true,
