{
  "workflow": {
    "root": null,
    "nodes": {
      "baseParameters": {
        "typeExecution": "parameters",
        "WebGUI": {
          "formData": {
            "sourceDir": "/home/blah1"
          }
        },
        "parameters" : {
          "basicPath" : "Hello"
        },
        "input_parameters": {
        }
      },
      "node1": {
        "typeExecution": "copyDir",
        "input_parameters": {
          "source": "{baseParameters.WebGUI.formData.sourceDir}",
          "target": "{WebGUI.formData.target}",
          "tester": "{parameters.name}"
        },
        "parameters": {
            "name"  : "good"
        },
        "WebGUI": {
          "formData": {
            "target": "/home/blah2"
          }
        }
      },
      "node2": {
        "typeExecution": "executeScript",
        "input_parameters": {
          "executeDir": "{node1.input_parameters.target}",
          "execute": "runMe {node1.WebGUI.formData.target} {node1.output.copyDir}",
          "father" : "{node1.baseParameters.parameters.basicPath}"
        }
      },
      "node3": {
        "typeExecution": "transformTemplate",
        "input_parameters": {
          "templates" : "{node2.input_parameters.executeDir}",
          "parameters": "{node1.input_parameters.target}"
        }
      }
    }
  }
}
